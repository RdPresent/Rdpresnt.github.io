  <script async='async' expr:src='data:post.commentSrc' type='text/javascript'/>\par
\par
  <script type='text/javascript'>\par
    (function() \{\par
      var items = <data:post.commentJso/>;\par
      var msgs = <data:post.commentMsgs/>;\par
      var config = <data:post.commentConfig/>;\par
\par
// <![CDATA[\par
      var cursor = null;\par
      if (items && items.length > 0) \{\par
        cursor = parseInt(items[items.length - 1].timestamp) + 1;\par
      \}\par
\par
      var bodyFromEntry = function(entry) \{\par
        var text = (entry &&\par
                    ((entry.content && entry.content.$t) ||\par
                     (entry.summary && entry.summary.$t))) ||\par
            '';\par
        if (entry && entry.gd$extendedProperty) \{\par
          for (var k in entry.gd$extendedProperty) \{\par
            if (entry.gd$extendedProperty[k].name == 'blogger.contentRemoved') \{\par
              return '<span class="deleted-comment">' + text + '</span>';\par
            \}\par
          \}\par
        \}\par
        return text;\par
      \}\par
\par
      var parse = function(data) \{\par
        cursor = null;\par
        var comments = [];\par
        if (data && data.feed && data.feed.entry) \{\par
          for (var i = 0, entry; entry = data.feed.entry[i]; i++) \{\par
            var comment = \{\};\par
            // comment ID, parsed out of the original id format\par
            var id = /blog-(\\d+).post-(\\d+)/.exec(entry.id.$t);\par
            comment.id = id ? id[2] : null;\par
            comment.body = bodyFromEntry(entry);\par
            comment.timestamp = Date.parse(entry.published.$t) + '';\par
            if (entry.author && entry.author.constructor === Array) \{\par
              var auth = entry.author[0];\par
              if (auth) \{\par
                comment.author = \{\par
                  name: (auth.name ? auth.name.$t : undefined),\par
                  profileUrl: (auth.uri ? auth.uri.$t : undefined),\par
                  avatarUrl: (auth.gd$image ? auth.gd$image.src : undefined)\par
                \};\par
              \}\par
            \}\par
            if (entry.link) \{\par
              if (entry.link[2]) \{\par
                comment.link = comment.permalink = entry.link[2].href;\par
              \}\par
              if (entry.link[3]) \{\par
                var pid = /.*comments\\/default\\/(\\d+)\\?.*/.exec(entry.link[3].href);\par
                if (pid && pid[1]) \{\par
                  comment.parentId = pid[1];\par
                \}\par
              \}\par
            \}\par
            comment.deleteclass = 'item-control blog-admin';\par
            if (entry.gd$extendedProperty) \{\par
              for (var k in entry.gd$extendedProperty) \{\par
                if (entry.gd$extendedProperty[k].name == 'blogger.itemClass') \{\par
                  comment.deleteclass += ' ' + entry.gd$extendedProperty[k].value;\par
                \} else if (entry.gd$extendedProperty[k].name == 'blogger.displayTime') \{\par
                  comment.displayTime = entry.gd$extendedProperty[k].value;\par
                \}\par
              \}\par
            \}\par
            comments.push(comment);\par
          \}\par
        \}\par
        return comments;\par
      \};\par
\par
      var paginator = function(callback) \{\par
        if (hasMore()) \{\par
          var url = config.feed + '?alt=json&v=2&orderby=published&reverse=false&max-results=50';\par
          if (cursor) \{\par
            url += '&published-min=' + new Date(cursor).toISOString();\par
          \}\par
          window.bloggercomments = function(data) \{\par
            var parsed = parse(data);\par
            cursor = parsed.length < 50 ? null\par
                : parseInt(parsed[parsed.length - 1].timestamp) + 1\par
            callback(parsed);\par
            window.bloggercomments = null;\par
          \}\par
          url += '&callback=bloggercomments';\par
          var script = document.createElement('script');\par
          script.type = 'text/javascript';\par
          script.src = url;\par
          document.getElementsByTagName('head')[0].appendChild(script);\par
        \}\par
      \};\par
      var hasMore = function() \{\par
        return !!cursor;\par
      \};\par
      var getMeta = function(key, comment) \{\par
        if ('iswriter' == key) \{\par
          var matches = !!comment.author\par
              && comment.author.name == config.authorName\par
              && comment.author.profileUrl == config.authorUrl;\par
          return matches ? 'true' : '';\par
        \} else if ('deletelink' == key) \{\par
          return config.baseUri + '/delete-comment.g?blogID='\par
               + config.blogId + '&postID=' + comment.id;\par
        \} else if ('deleteclass' == key) \{\par
          return comment.deleteclass;\par
        \}\par
        return '';\par
      \};\par
\par
      var replybox = null;\par
      var replyUrlParts = null;\par
      var replyParent = undefined;\par
\par
      var onReply = function(commentId, domId) \{\par
        if (replybox == null) \{\par
          // lazily cache replybox, and adjust to suit this style:\par
          replybox = document.getElementById('comment-editor');\par
          if (replybox != null) \{\par
            replybox.height = '250px';\par
            replybox.style.display = 'block';\par
            replyUrlParts = replybox.src.split('#');\par
          \}\par
        \}\par
        if (replybox && (commentId !== replyParent)) \{\par
          replybox.src = '';\par
          document.getElementById(domId).insertBefore(replybox, null);\par
          replybox.src = replyUrlParts[0]\par
              + (commentId ? '&parentID=' + commentId : '')\par
              + '#' + replyUrlParts[1];\par
          replyParent = commentId;\par
        \}\par
      \};\par
\par
      var hash = (window.location.hash || '#').substring(1);\par
      var startThread, targetComment;\par
      if (/^comment-form_/.test(hash)) \{\par
        startThread = hash.substring('comment-form_'.length);\par
      \} else if (/^c[0-9]+$/.test(hash)) \{\par
        targetComment = hash.substring(1);\par
      \}\par
\par
      // Configure commenting API:\par
      var configJso = \{\par
        'maxDepth': config.maxThreadDepth\par
      \};\par
      var provider = \{\par
        'id': config.postId,\par
        'data': items,\par
        'loadNext': paginator,\par
        'hasMore': hasMore,\par
        'getMeta': getMeta,\par
        'onReply': onReply,\par
        'rendered': true,\par
        'initComment': targetComment,\par
        'initReplyThread': startThread,\par
        'config': configJso,\par
        'messages': msgs\par
      \};\par
\par
      var render = function() \{\par
        if (window.goog && window.goog.comments) \{\par
          var holder = document.getElementById('comment-holder');\par
          window.goog.comments.render(holder, provider);\par
        \}\par
      \};\par
\par
      // render now, or queue to render when library loads:\par
      if (window.goog && window.goog.comments) \{\par
        render();\par
      \} else \{\par
        window.goog = window.goog || \{\};\par
        window.goog.comments = window.goog.comments || \{\};\par
        window.goog.comments.loadQueue = window.goog.comments.loadQueue || [];\par
        window.goog.comments.loadQueue.push(render);\par
      \}\par
    \})();\par
// ]]>\par
  </script>\par
